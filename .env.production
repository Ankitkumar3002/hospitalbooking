CI=false
# Professional Environment Configuration

# API Configuration
REACT_APP_API_URL=https://api.jagjeevan-hospital.com
REACT_APP_SOCKET_URL=wss://api.jagjeevan-hospital.com

# SMS Service (MSG91)
REACT_APP_MSG91_AUTH_KEY=your_msg91_auth_key
REACT_APP_MSG91_TEMPLATE_ID=your_template_id
REACT_APP_MSG91_SENDER_ID=JAGHOS

# Alternative: Twilio
REACT_APP_TWILIO_ACCOUNT_SID=your_twilio_account_sid
REACT_APP_TWILIO_AUTH_TOKEN=your_twilio_auth_token
REACT_APP_TWILIO_PHONE_NUMBER=+1234567890

# Payment Gateway (Razorpay)
REACT_APP_RAZORPAY_KEY_ID=rzp_live_your_key_id
REACT_APP_RAZORPAY_KEY_SECRET=your_secret_key

# Database
REACT_APP_DB_URL=mongodb://localhost:27017/hospital_booking
REACT_APP_REDIS_URL=redis://localhost:6379

# Security
REACT_APP_JWT_SECRET=your_jwt_secret_key
REACT_APP_ENCRYPTION_KEY=your_encryption_key

# Google Maps (for location services)
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Firebase (for notifications)
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key
REACT_APP_FIREBASE_PROJECT_ID=your_project_id

# Analytics
REACT_APP_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX

# Feature Flags
REACT_APP_ENABLE_VIDEO_CONSULTATION=true
REACT_APP_ENABLE_PRESCRIPTION_UPLOAD=true
REACT_APP_ENABLE_HEALTH_RECORDS=true
